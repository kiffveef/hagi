{
  "mcpServers": {
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "CONTEXT7_API_KEY": ""
      }
    },
    "one-search": {
      "disabled": true,
      "command": "npx",
      "args": [
        "-y",
        "one-search-mcp"
      ],
      "env": {
        "SEARCH_PROVIDER": "duckduckgo"
      }
    },
    "memory": {
      "disabled": true,
      "command": "npx",
      "args": [
        "-y",
        "@iachilles/memento@latest"
      ],
      "env": {
        "MEMORY_DB_PATH": "${XDG_DATA_HOME:-$HOME/.local/share}/claude-memory/memory.db"
      }
    },
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git",
        "--repository",
        "."
      ],
      "disabled": true
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "claude-code"
      ],
      "disabled": true,
      "env": {
        "SERENA_CACHE_DIR": "${XDG_CACHE_HOME:-$HOME/.cache}/serena"
      }
    }
  }
}
